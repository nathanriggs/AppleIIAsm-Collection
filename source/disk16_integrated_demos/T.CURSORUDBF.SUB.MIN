CURSFOR
         CLC
         ADC   CURSH
         STA   CURSH
         LDA   ERRCTRL
         CMP   #1
         BEQ   :CHKERR
         JMP   :ERREND
:CHKERR
         LDA   CURSH
         CMP   #40        ; MAX COL + 1
         BCS   :XOVF
         JMP   :ERREND
:XOVF
         _ERR  :E_SID;:E_XOF;:E_DMP1;RETURN;#10
:ERREND
         JSR   VTAB
         RTS
:E_SID   ASC   "CURSFOR (CURF MACRO)",00
:E_DMP1  ASC   "DUMPING RETURN BLOCK...",00
:E_XOF   ASC   "CURSOR X OVERFLOW: X <> MIN/MAX",00
